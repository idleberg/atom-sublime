{
  "name": "sublime",
  "version": "0.5.0",
  "description": "A meta-package to bring some of Sublime Text's features to Atom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-sublime"
  },
  "keywords": [
    "sublime text",
    "sublimetext"
  ],
  "main": "./lib/sublime",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "package-deps": [
    "atom-wrap-in-tag",
    "auto-update-plus",
    "browse",
    "buildium",
    "ctrl-dir-scroll",
    "minimap",
    "multi-cursor",
    "satisfy-dependencies",
    "set-syntax",
    "sublime-block-comment",
    "sublime-style-column-selection",
    "sublime-word-navigation",
    "sublimify",
    "tag",
    "text-manipulation"
  ],
  "dependencies": {
    "atom-package-deps": "^4.4.1"
  },
  "devDependencies": {
    "husky": "^0.14.3"
  }
}
